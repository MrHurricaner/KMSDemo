<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/tv_shared_owner"
        style="@style/Text.a9b0ca.14"
        android:text="Owner1" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="@drawable/bg_payment_wallet_info"
        android:paddingBottom="16dp"
        android:paddingTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/tv_shared_owner">

        <TextView
            android:id="@+id/tv_scan_shared_owner"
            style="@style/Text.f0f4fd.14"
            android:drawableLeft="@drawable/icon_shared_owner_scan"
            android:drawablePadding="8dp"
            android:text="@string/scan_shared_owner_qr_code"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>
